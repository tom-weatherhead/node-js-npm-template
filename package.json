{
	"name": "node-js-npm-template",
	"description": "A Node.js / npm project template",
	"version": "0.0.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tom-weatherhead/node-js-npm-template.git"
	},
	"homepage": "https://github.com/tom-weatherhead/node-js-npm-template#readme",
	"bugs": {
		"url": "https://github.com/tom-weatherhead/node-js-npm-template/issues"
	},
	"main": "src/main",
	"scripts": {
		"build": "npm i && grunt",
		"clean": "rm -rf node_modules && rm -f package-lock.json",
		"jest-coverage": "jest --coverage",
		"start": "grunt",
		"test": "grunt",
		"test-jest": "jest"
	},
	"jest": {
		"testMatch": [
			"<rootDir>/test/jest/*_jest.js"
		]
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"grunt": "^1.0.4",
		"grunt-eslint": "^22.0.0",
		"grunt-mocha-test": "^0.13.3",
		"grunt-run": "^0.8.1",
		"jest": "^24.8.0",
		"mocha": "^10.2.0"
	},
	"engineStrict": true,
	"engines": {
		"node": ">= 8"
	},
	"keywords": [
		"node",
		"npm"
	],
	"author": "Tom Weatherhead <null@2hrd4u.org> (https://httpbin.org/status/418)",
	"license": "MIT"
}
